<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle` and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
			{#{{ encore_entry_link_tags('app') }}#}
			{{ encore_entry_link_tags('bootstrap') }}
			{{ encore_entry_link_tags('custom') }}
		{% endblock %}

		{% block javascripts %}
			{#{{ encore_entry_script_tags('app') }}#}
			<link rel="preconnect" href="https://fonts.googleapis.com">
			<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
			<link href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&display=swap" rel="stylesheet">
		{% endblock %}
	</head>
	<body>
		{% include('navbar.html.twig') %}

		<div class="row" style="width: 90%; margin: auto;">
			<div class="col-md-9"> {% block body %}{% endblock %}
				</div>
				<div class="col-md-3"></div>
			</div>
		</body>
	</html>
