<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle` and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
			{#{{ encore_entry_link_tags('app') }}#}
			{{ encore_entry_link_tags('bootstrap') }}
			{{ encore_entry_link_tags('custom') }}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.3.1/styles/atom-one-dark-reasonable.min.css">
		{% endblock %}

		{% block javascripts %}
			{#{{ encore_entry_script_tags('app') }}#}
			<link rel="preconnect" href="https://fonts.googleapis.com">
			<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
			<link href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&display=swap" rel="stylesheet">
			<script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/11.3.1/highlight.min.js"></script>
		{% endblock %}
	</head>
	<body>
		<header>
			<div class="container">
				<div class="site-branding">
					<h1 class="site-title">
						<a href="#">Jerem Dev</a>
					</h1>
					<p class="site-description">Caf√© et code, pas toujours dans cet ordre</p>
				</div>
			</div>
		</header>

		<div class="row" style="width: 90%; margin: auto;">
			{% include('navbar.html.twig') %}
			{% block body %}{% endblock %}
		</div>

	</body>
</html>
